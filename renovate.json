{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "prConcurrentLimit": 20,
  "prHourlyLimit": 20,
  "packageRules": [
    {
      "groupName": "Next.js and React.js major",
      "matchPackageNames": [
        "next",
        "react"
      ],
      "matchUpdateTypes": ["major"],
      "enabled": false
    }
  ]
}
